<widget type="JMSSourceWidget" label="i18n:jmsSource-title">
  <description>i18n:jmsSource-desc</description>
  <metadata>
    <size width="80" height="40" type="fixed"/>
    <property frame="RoundedBox" visible="Never" visual="false" inspectable="true"/>
  </metadata>
  <inputs>
    <param binding="false" type="Enumeration" label="i18n:jms-location" name="location" value="LOCAL"
        showButton="true" showButtonValue="EXTERNAL" 
        popupImplClass="com.vitria.opbook.widgets.items.EnumerationItemPopupJMS">
       <description>i18n:jms-location-desc</description>
       <choice label="i18n:jms-local" data="LOCAL"/>
       <choice label="i18n:jms-external" data="EXTERNAL"/>
    </param>
    <param binding="false" type="Enumeration" label="i18n:jms-type" name="type" value="TOPIC">
       <description>i18n:jms-type-desc</description>
       <choice label="i18n:jms-topic" data="TOPIC"/>
       <choice label="i18n:jms-queue" data="QUEUE"/>
    </param>
	<param binding="false" label="i18n:jms-connetion-factory" name="connfactory" value="">
       <description>i18n:jms-location-desc</description>
    </param>
    <param binding="false" label="i18n:jms-name" name="destination">
       <description>i18n:jms-name-desc</description>
    </param>
    <param binding="false" label="i18n:jms-username" name="username">
       <description>i18n:jms-username-desc</description>
    </param>
    <param binding="false" label="i18n:jms-password" type="password" name="password">
       <description>i18n:jms-password-desc</description>
    </param>
	
    <param binding="false" label="i18n:jmsSource-selector" name="selector">
       <description>i18n:jmsSource-selector-desc</description>
    </param>
    <param binding="false" value="0" type="number" step="1" label="i18n:jms-max-count" name="maxcount" min="0" max="999999">
      <description>i18n:jms-max-count-desc</description>
    </param>
    
  </inputs>
  <outputs>
    <param label="i18n:jmsSource-output" name="$result" setting="false">
    	<description>i18n:jmsSource-output-desc</description>
    </param>
  </outputs>
</widget>
