		<size width="80" height="40" type="fixed"/>
		<property frame="RoundedBox" visible="Never" visual="false" refreshable="false"/>
    </metadata>
    <inputs>
        <param label="i18n:query-list-widget-query-limitation" name="limitation" type="number" binding="false" control="false">
