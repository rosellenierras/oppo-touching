    <metadata>
        <size width="80" height="40" type="fixed"/>
        <property frame="RoundedBox" visible="Never" visual="false" refreshable="false"/>
    </metadata>
	<inputs>
        <param binding="true" setting="false" label="i18n:string-replacement-input" name="input">
           <description>i18n:string-replacement-input-desc</description>
