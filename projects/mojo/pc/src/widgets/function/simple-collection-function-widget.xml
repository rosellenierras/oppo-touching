<widget type="SimpleCollectionFunctionWidget" label="i18n:simpleCollectionFunction-title">
  <description>i18n:simpleCollectionFunction-description</description>
  <metadata>
    <size width="80" height="40" type="fixed"/>
    <property frame="RoundedBox" visible="Never" visual="false" refreshable="false"/>
  </metadata>
  <inputs>
    <param control="false" binding="false" type="Enumeration" value="SUM" label="i18n:simpleCollectionFunction-function" name="$operation">
      <description>i18n:simpleCollectionFunction-operation-desc</description>
      <choice label="i18n:simpleCollectionFunction-sum" data="SUM"/>
      <choice label="i18n:simpleCollectionFunction-average" data="AVERAGE"/>
      <choice label="i18n:complexCollectionFunction-count" data="COUNT"/>
      <choice label="i18n:simpleCollectionFunction-median" data="MEDIAN"/>
      <choice label="i18n:simpleCollectionFunction-mode" data="MODE"/>
      <choice label="i18n:simpleCollectionFunction-maximum" data="MAX"/>
      <choice label="i18n:simpleCollectionFunction-minimum" data="MIN"/>
    </param>
    <param control="false" setting="false" label="i18n:simpleCollectionFunction-collection" name="$arg">
       <description>i18n:simpleCollectionFunction-input-desc</description>
    </param>
  </inputs>
  <outputs>
    <param setting="false" label="i18n:simpleCollectionFunction-result" name="$result">
       <description>i18n:simpleCollectionFunction-result-desc</description>
    </param>
  </outputs>
</widget>
