<widget type="JoinRecordWidget" label="i18n:join-record-title">
	<description>i18n:join-record-description</description>
	<metadata>
		<size width="80" height="40" type="fixed"/>
		<property frame="RoundedBox" visible="Never" visual="false" refreshable="false"/>
	</metadata>
	<inputs>
		<param control="false" setting="false" label="i18n:left-record" name="leftRecord">
		   <description>i18n:left-record-desc</description>
		</param>
		<param control="false" setting="false" label="i18n:right-record" name="rightRecord">
		   <description>i18n:right-record-desc</description>
		</param>
		<param type="aliannamegrid" label="i18n:alien-names" name="alienName" binding="false"
			dataProvider="left-fields;right-fields">
		   <description>i18n:alien-names-desc</description>
		</param>		
		<param type="Enumeration" label="i18n:join-type" name="joinType" binding="false"
			value="inner">
		  <description>i18n:join-type-desc</description>
		  <choice label="i18n:inner-join" data="inner"/>
		  <choice label="i18n:left-join" data="left"/>
		  <choice label="i18n:full-join" data="full"/>
		</param>
		<param binding="false" type="joinongrid" label="i18n:join-on" name="joinOn"
			dataProvider="left-fields;right-fields">
		   <description>i18n:join-on-desc</description>
		</param>		
	</inputs>
  
	<outputs>
		<param setting="false" label="i18n:join-result" name="$result">
		  <description>i18n:join-result-desc</description>
		</param>
	</outputs>
  
</widget>
