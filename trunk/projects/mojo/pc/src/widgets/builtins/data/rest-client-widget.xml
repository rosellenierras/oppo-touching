<widget type="RestClientWidget" label="i18n:restClient-title">
  <description>
  i18n:restClient-desc
  </description>
  <metadata>
    <property frame="RoundedBox" visible="Never" visual="false" inspectable="true" actionable="true"/>
  </metadata>  
  <inputs>
    <param type="Enumeration" label="i18n:restClient-service" name="rService" binding="false"
    	dataProvider="rest-service-list" control="false">
      <description>i18n:restClient-service-desc</description>
      <choice label="Google base" data="gbase"/>
      <choice label="Amzon ECS" data="ecs"/>       
    </param>
    <param type="string" label="i18n:restClient-url" name="url">
       <description>i18n:restClient-url-desc</description>
    </param>	
    <param type="string" label="i18n:restClient-user-name" name="userName">
       <description>i18n:restClient-user-name-desc</description>
    </param>
    <param type="password" label="i18n:restClient-password" name="password">
       <description>i18n:restClient-password-desc</description>
    </param>
    <param type="rawtext" label="i18n:restClient-auth-option" name="authOption">
       <description>i18n:restClient-auth-option-desc</description>
    </param>
    <param type="multifieldgrid" label="i18n:restClient-request-parameters" name="reqParams"
		dataProvider="rest-param-choices">
       <description>i18n:restClient-request-parameters-desc</description>
    </param>
    <param type="string" label="i18n:restClient-encoding" name="encoding" control="false">
       <description>i18n:restClient-encoding-desc</description>
    </param>   
  </inputs>
  <outputs>
    <param type="string" label="i18n:restClient-data" name="$result" setting="false">
       <description>i18n:restClient-data-desc</description>
    </param>
  </outputs>
</widget>
